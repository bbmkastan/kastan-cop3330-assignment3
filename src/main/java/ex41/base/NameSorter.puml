@startuml
!define DARKBLUE
!includeurl https://raw.githubusercontent.com/Drakemor/RedDress-PlantUML/master/style.puml
'https://plantuml.com/class-diagram

class App {
+ main()
+ generateOutputString()
+ printOutput()
}

class nameSorter {
- listOfNames: ArrayList

+ sort()
+ getSortedNames()
}

class FileReader {

+ getStrings()
+ readFile()
}

App o--> nameSorter
nameSorter o--> FileReader
@enduml